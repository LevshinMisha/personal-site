{
  "name": "framework-comparison",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./src/server/index.js",
    "prod": "cross-env NODE_ENV=production babel-node ./src/server/index.js",
    "build-webpack": "rmdir-cli build && webpack --config webpack.prod.js",
    "build-parcel": "rmdir-cli build && parcel build ./src/**/*.html --no-source-maps -d build",
    "build-gulp": "rmdir-cli build && gulp build",
    "env-build-gulp": "gulp clear-build build:env --series",
    "watch-webpack": "cross-env NODE_ENV=dev babel-node ./src/server/index.js",
    "watch-parcel": "parcel watch ./src/**/*.html -d build/parcel --public-url /parcel",
    "watch-gulp": "gulp build watch --series"
  },
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-import": {},
      "cssnano": {}
    }
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "./src/common"
  },
  "browserslist": ">0%",
  "dependencies": {
    "@babel/node": "7.2.2",
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
